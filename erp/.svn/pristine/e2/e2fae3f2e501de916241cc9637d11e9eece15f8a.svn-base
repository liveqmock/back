package com.ihk.report.data.pojo.commission;

import java.util.Date;
import java.math.BigDecimal;
import java.io.Serializable;

/**
 * 佣金：公司
 * @author 
 *
 */
public class ReportCommissionCompany implements Serializable{
	private static final long serialVersionUID = 1L;


	private int companyId;	//公司id
	private String companyName;	//公司名称
	private BigDecimal paymentAmount;	//已收佣金
	private BigDecimal notgetAmount;	//未收佣金


	/**
	 * 公司id
	 * @return
	 */
	public int getCompanyId() {
		return companyId;
	}
	/**
	 * 公司id
	 * @param companyId
	 */
	public void setCompanyId(int companyId) {
		this.companyId = companyId;
	}

	/**
	 * 公司名称
	 * @return
	 */
	public String getCompanyName() {
		return companyName;
	}
	/**
	 * 公司名称
	 * @param companyName
	 */
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}

	/**
	 * 已收佣金
	 * @return
	 */
	public BigDecimal getPaymentAmount() {
		return paymentAmount;
	}
	/**
	 * 已收佣金
	 * @param paymentAmount
	 */
	public void setPaymentAmount(BigDecimal paymentAmount) {
		this.paymentAmount = paymentAmount;
	}

	/**
	 * 未收佣金
	 * @return
	 */
	public BigDecimal getNotgetAmount() {
		return notgetAmount;
	}
	/**
	 * 未收佣金
	 * @param notgetAmount
	 */
	public void setNotgetAmount(BigDecimal notgetAmount) {
		this.notgetAmount = notgetAmount;
	}	
    
}
