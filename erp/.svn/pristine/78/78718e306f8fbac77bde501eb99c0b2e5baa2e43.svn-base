	public String getPropertyProjectName(){
		
		return DescUtils.findPropertyNameByAppointId(this.getAppointId());
	}
	
	public String getWriterName(){
		
		String saleName = DescUtils.getUserRealName(this.getWriterId());
		
		return saleName == null ? "" : saleName;
	}
	
	public String getApprovalUserName(){
		
		String saleName = DescUtils.getUserRealName(this.getApprovalUserid());
		
		return saleName == null ? "" : saleName;
	}
	
	public String getBillTypeStr(){
		
		return DescUtils.getCodeDesc(EnumCodeTypeName.SALEUNIT_BILL_TYPE, this.getBillType(), ContProjectId.GUANG_ZHOU);
	}
	
	//主要用于显示,从实收款项中求和
	public BigDecimal getPayMoney() {
		
		BigDecimal ret = new BigDecimal(0);
		
		List<AppointBillDetail> detailList = DescUtils.getAppointBillDetailServices().findDetailByAppointBillId(this.getId());
		if(!CommonUtils.isCollectionEmpty(detailList)){
			
			for(AppointBillDetail detail : detailList){
				
				ret = ret.add(detail.getPayMoney());
			}
		}
		
		return ret;
	} 
	
