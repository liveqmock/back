package com.ihk.constanttype;

/**
 * 表project_text
 * @author dtc
 * 2013-6-7,下午04:04:22
 */
public enum EnumProjectTextType {
	
	/**
	 * 付款方式
	 */
	PAY_WAY(1, "pay_way");
	
	/**
	 * 类型
	 */
	private int type;
	
	/**
	 * 对应的表名
	 */
	private String value;
	
	public void setType(int type) {
		this.type = type;
	}
	
	public int getType() {
		return type;
	}
	
	public void setValue(String value) {
		this.value = value;
	}
	
	public String getValue() {
		return value;
	}
	
	private EnumProjectTextType(int type, String value){
		this.type = type;
		this.value = value;
	}
	
}
