	/**
	 * 获取客户类型的中文
	 */
	public String getConfirmTypeStr(){
		if(ContConfirmType.CONFIRM.equals(confirmType)){
			
			return ContConfirmType.CONFIRM_STR_CN;
		}else if(ContConfirmType.CONTRACT.equals(confirmType)){
			
			return ContConfirmType.CONTRACT_STR_CN;
		}else if(ContConfirmType.CHIP.equals(confirmType)){
			
			return ContConfirmType.CHIP_STR_CN;
		}else if(ContConfirmType.CONFIRM_BOOK.equals(confirmType)){
			
			return ContConfirmType.CONFIRM_BOOK_STR_CN;
		}else if(ContConfirmType.CUSTOMER.equals(confirmType)){
			
			return ContConfirmType.CUSTOMER_STR_CN;
		}
		
		return "未知客户类型";
	}
	
