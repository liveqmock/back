package com.ihk.property.data.pojo;

import com.ihk.utils.SearchCond;

/**
 * PropertyGroupCond的查询条件
 * @author peter.kuang
 *
 */
public class PropertyGroupCond extends SearchCond{

	private static final long serialVersionUID = 1L;
	
	private String projectId;	
    private String groupName;
    private int companyProject;
    
	public int getCompanyProject() {
		return companyProject;
	}

	public void setCompanyProject(int companyProject) {
		this.companyProject = companyProject;
	}

	public String getGroupName() {
		return groupName;
	}

	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}

	public String getProjectId() {
		return projectId;
	}

	public String getSearchName() {
		return projectId;
	}

	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}
}
