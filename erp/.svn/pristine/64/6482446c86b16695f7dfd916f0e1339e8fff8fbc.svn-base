package com.ihk.report.data.pojo.unitsale;

import java.util.Date;
import java.math.BigDecimal;
import java.io.Serializable;

/**
 * 月销量：销售人员
 * @author 
 *
 */
public class ReportUnitSaleSalesman implements Serializable{
	private static final long serialVersionUID = 1L;


	private String salesmanId;	//销售人员id
	private String salesmanName;	//销售人员名称
	private BigDecimal sumMoney;	//汇总金额
	private BigDecimal sumArea;	//汇总面积
	private BigDecimal sumUnit;	//汇总套数
	private String endDate;	//截止日期


	/**
	 * 销售人员id
	 * @return
	 */
	public String getSalesmanId() {
		return salesmanId;
	}
	/**
	 * 销售人员id
	 * @param salesmanId
	 */
	public void setSalesmanId(String salesmanId) {
		this.salesmanId = salesmanId;
	}

	/**
	 * 销售人员名称
	 * @return
	 */
	public String getSalesmanName() {
		return salesmanName;
	}
	/**
	 * 销售人员名称
	 * @param salesmanName
	 */
	public void setSalesmanName(String salesmanName) {
		this.salesmanName = salesmanName;
	}

	/**
	 * 汇总金额
	 * @return
	 */
	public BigDecimal getSumMoney() {
		return sumMoney;
	}
	/**
	 * 汇总金额
	 * @param sumMoney
	 */
	public void setSumMoney(BigDecimal sumMoney) {
		this.sumMoney = sumMoney;
	}

	/**
	 * 汇总面积
	 * @return
	 */
	public BigDecimal getSumArea() {
		return sumArea;
	}
	/**
	 * 汇总面积
	 * @param sumArea
	 */
	public void setSumArea(BigDecimal sumArea) {
		this.sumArea = sumArea;
	}

	/**
	 * 汇总套数
	 * @return
	 */
	public BigDecimal getSumUnit() {
		return sumUnit;
	}
	/**
	 * 汇总套数
	 * @param sumUnit
	 */
	public void setSumUnit(BigDecimal sumUnit) {
		this.sumUnit = sumUnit;
	}

	/**
	 * 截止日期
	 * @return
	 */
	public String getEndDate() {
		return endDate;
	}
	/**
	 * 截止日期
	 * @param endDate
	 */
	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	
    
}
