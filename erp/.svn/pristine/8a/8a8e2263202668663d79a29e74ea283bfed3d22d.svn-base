/*--font--*/
a:link {color:#777; text-decoration:none;}
a:visited {color:#777; text-decoration:none;}
a:hover {color:#777; text-decoration:underline;}
.fontblue{ color:#5482de;}
.fontblue a:link{color:#5482de; text-decoration:none;}
.fontblue a:visited{color:#5482de; text-decoration:none;}
.fontblue a:hover{color:#5482de; text-decoration:none;}

/*--menu--*/
.nav {
height:32px; 
width:100%; 
z-index:100;
padding:0;
}

.nav .table {
display:table; 
margin:0;
}

.nav .select,
.nav .current {
margin:0; 
padding:0; 
list-style:none; 
display:table-cell; 
white-space:nowrap;
}

.nav li {
margin:0; 
padding:0; 
height:32px; 
float:left;
}

.nav .select a {
display:block; 
height:32px; 
float:left; 
font-weight:bold;
padding:0 10px 0 10px; 
text-decoration:none; 
line-height:32px; 
white-space:nowrap; 
color:#fff;
}


.nav .select a:hover, 
.nav .select li:hover a {
background:#202020; 
padding:0px; 
cursor:pointer; 
color:#fdfa00;
}

.nav .select a b{
	font-weight:bold;
	}

.nav .select a:hover b, 
.nav .select li:hover a b {
display:block; 
float:left; 
padding:0 10px 0 10px; 
background:none; 
cursor:pointer;
}

.nav .select_sub {
display:none;
}

/* IE6 only */
.nav table {
border-collapse:collapse; 
margin:-1px; 
font-size:1em; 
width:0; 
height:0;
}

.nav .sub {
display:table; 
margin:0; 
padding:0; 
list-style:none;
font-size:12px;
}

.nav .sub_active .current_sub a, 
.nav .sub_active a:hover {
background:transparent; 
color:#fff;
}

.nav .select :hover .select_sub, 
.nav .current .show {
display:block; 
position:absolute; 
width:100%; 
top:116px; 
background:none; 
padding:0; 
z-index:100; 
left:0; 
text-align:left;
}

.nav .current .show {
z-index:10;
}

.nav .select :hover .sub li a, 
.nav .current .show .sub li a {
display:block; 
float:left; 
background:none; 
padding:0 5px 0 5px; 
margin:5px;
white-space:nowrap; 
border:0; 
color:#fff;
}

.nav .current .sub li.sub_show a {
color:#fff; 
cursor:default; 
}

.nav .select .sub li a {
	font-weight:normal;
}

.nav .select :hover .sub li a:hover, 
.nav .current .sub li a:hover {
visibility:visible; 
color:#fdfa00; 
}
