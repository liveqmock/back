package com.ihk.property.data.pojo;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 销售中心-收款情况-使用公式,要用到的类,非数据库中的表
 * @author dtc
 * 2013-6-26,上午11:28:58
 */
public class MyPayWay implements Serializable{

	private static final long serialVersionUID = 9124079645960494364L;
	
	/**
	 * 对应的单元
	 */
	private int unitId;
	
	/**
	 * 付款方式
	 */
	private int payWayId;
	
	/**
	 * 设置方式
	 */
	private int formula;
	
	/**
	 * 按揭比例(成)
	 */
	private int proportion;
	
	/**
	 * 首期比例(百分比)
	 */
	private BigDecimal firstInstallment;
	
	/**
	 * 按揭比例(百分比)
	 */
	private BigDecimal mortgage;
	
	/**
	 * 首期金额
	 */
	private BigDecimal firstInstallmentMoney;
	
	/**
	 * 按揭金额
	 */
	private BigDecimal mortgageMoney;
	
	public int getUnitId() {
		return unitId;
	}
	
	public void setUnitId(int unitId) {
		this.unitId = unitId;
	}

	public int getPayWayId() {
		return payWayId;
	}
	
	public void setPayWayId(int payWayId) {
		this.payWayId = payWayId;
	}

	public int getFormula() {
		return formula;
	}

	public void setFormula(int formula) {
		this.formula = formula;
	}

	public int getProportion() {
		return proportion;
	}

	public void setProportion(int proportion) {
		this.proportion = proportion;
	}

	public BigDecimal getFirstInstallment() {
		return firstInstallment;
	}

	public void setFirstInstallment(BigDecimal firstInstallment) {
		this.firstInstallment = firstInstallment;
	}

	public BigDecimal getMortgage() {
		return mortgage;
	}

	public void setMortgage(BigDecimal mortgage) {
		this.mortgage = mortgage;
	}

	public BigDecimal getFirstInstallmentMoney() {
		return firstInstallmentMoney;
	}

	public void setFirstInstallmentMoney(BigDecimal firstInstallmentMoney) {
		this.firstInstallmentMoney = firstInstallmentMoney;
	}

	public BigDecimal getMortgageMoney() {
		return mortgageMoney;
	}

	public void setMortgageMoney(BigDecimal mortgageMoney) {
		this.mortgageMoney = mortgageMoney;
	}
	
}
