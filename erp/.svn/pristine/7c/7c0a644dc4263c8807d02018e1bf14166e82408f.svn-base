	/**
	 * 页面显示的项目名,要判断项目,分区是否为0
	 * 
	 * @return
	 */
	public String getShowName() {

		if (buildId > 0) {

			return MyPropertyUtils.getPropertyBuildServices()
					.findPropertyBuildById(buildId).getAllName();

		} else if (areaId > 0) {

			String proName = MyPropertyUtils.getPropertyProjectServices()
				.findPropertyProjectById(propertyId).getPropertyName();
			String areaName = MyPropertyUtils.getPropertyAreaServices()
				.findPropertyAreaById(areaId).getAreaName();

			return proName + "-" + areaName;

		} else if (propertyId > 0) {

			return MyPropertyUtils.getPropertyProjectServices()
					.findPropertyProjectById(propertyId).getPropertyName();
		}

		return "";
	}

