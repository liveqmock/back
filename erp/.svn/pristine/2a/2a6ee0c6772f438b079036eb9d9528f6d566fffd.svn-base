<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" isELIgnored="false"%>
<%@ page contentType="text/html; charset=utf-8" %>
<%@ taglib prefix="s" uri="/struts-tags" %>
<%--这是一个显示房间详细信息的弹出框 --%>
	<div style="float: left;margin-left: 20px;border: solid 2px;border-color: gray;background-color: #eeeeee;padding: 10px">
				
					<table cellpadding="0" cellspacing="1"  style="vertical-align: top;" class="tb1 gbox">
					
				  
				   	
				   
   					<tr>
				   	<td>房间号</td><td> ${selectUnit. unitNo} </td>
				   	<td>楼层号</td><td> ${selectUnit. floorNum}</td>
				   </tr>	
				   

   					<tr>
				   	<td>建筑面积</td><td>${selectUnit.buildArea}</td>
				   	<td>套内面积</td><td> ${selectUnit.insideArea}</td>
				   </tr>	

   					<tr>
				   	<td>建筑单价</td><td>${selectUnit.buildPrice}</td>
				   	<td>套内单价</td><td></td>
				   </tr>	

   					<tr>
				   	<td>标准总价</td><td> ${selectUnit.sumPrice}</td>
				   	<td>计价方式</td><td>${selectUnit.priceWay}</td>
				   </tr>

   					<tr>
				   	<td>面积状态</td><td>${selectUnit.areaState}</td>
				   	<td>景观</td><td>${selectUnit.scenery}</td>
				   </tr>

   					<tr>
				   	<td>朝向</td><td>${selectUnit.orientation}</td>
				   	<td>预售建筑面积</td><td>${selectUnit.preBuildArea}</td>
				   </tr>

      					<tr>
				   	<td>预售套内面积</td><td> ${selectUnit.preInsideArea}</td>
				   	<td>实测建筑面积</td><td> ${selectUnit.realBuildArea}</td>
				   </tr>

      				<tr>
				   	<td>实测套内面积</td><td> ${selectUnit.realInsideArea}</td>
				   	<td></td><td> </td>
				   </tr>

      					<tr>
				   	<td>装修款</td><td> ${selectUnit.renovateMoney}</td>
				   	<td>是否样板房</td><td>
				   		${selectUnit.isSample}
				   	</td>
				   		
				   </tr>

      					<tr>
				   	<td>是否附属房产</td><td>
				   		${selectUnit.isSlave}
				   	</td>
				   	
                    <td> 抵押状态</td><td>${selectUnit.mortgageState }</td>
				   </tr>

      				<tr>
				   	<td>户型点评</td><td> ${selectUnit.houseDesc}</td>
				   	<td>装修标准</td><td> ${selectUnit.renovateDesc}</td>
				   </tr>

      					<tr>
				   	<td>装修单价</td><td>${selectUnit.renovatePrice}</td>
				   	<td>备注</td><td>${selectUnit.remark}</td>
				   </tr>
				
				  
				
		</table>
			
   
				<div style="width: 95%;float: left">
					<input type="submit" value="  选择  " id="selectUnitInformation"/>
					<input type="submit" value="  关闭  " id="closeUnitInformation"/>
				</div>
		</div>