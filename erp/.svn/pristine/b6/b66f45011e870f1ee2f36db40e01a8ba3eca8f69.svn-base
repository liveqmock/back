package com.ihk.saleunit.data.pojo;

import com.ihk.utils.SearchCond;

/**
 * QuestionAnwser的查询条件
 * @author peter.kuang
 *
 */
public class QuestionAnwserCond extends SearchCond{
	private static final long serialVersionUID = 1L;

	private String questionId;	
    
	private String preCustomerId;
	
	private String customerId;
	
	private String unitId;
	
	
	public String getPreCustomerId() {
		return preCustomerId;
	}

	public QuestionAnwserCond setPreCustomerId(String preCustomerId) {
		this.preCustomerId = preCustomerId;
		return this;
	}

	public String getSearchName() {
		return questionId;
	}

	public QuestionAnwserCond setQuestionId(String questionId) {
		this.questionId = questionId;
		return this;
	}

	public String getCustomerId() {
		return customerId;
	}

	public QuestionAnwserCond setCustomerId(String customerId) {
		this.customerId = customerId;
		return this;
	}

	public String getUnitId() {
		return unitId;
	}

	public QuestionAnwserCond setUnitId(String unitId) {
		this.unitId = unitId;
		return this;
	}

	public String getQuestionId() {
		return questionId;
	}
	
	
	
}
