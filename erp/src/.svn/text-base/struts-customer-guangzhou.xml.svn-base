<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<!-- Customer(客户) action -->
	<package name="customer_guangzhou" extends="struts-default">
		<action name="customer_guangzhou!*" class="com.ihk.customer.action.CustomerAction">
			<result name="customerAdd" type="redirectAction">
				customer_guangzhou!doSomeForAddCustomer?from=guangzhou&amp;deleteSession=false
			</result>
			<result name="loginFail" type="redirect">/login.jsp</result>
			<result name="loginSucc">/customer/guangzhou/index.jsp</result>
			<result name="queryCustomerLimit">/customer/guangzhou/index.jsp</result>
			<result name="queryCustomerByIdForUpdate">/customer/guangzhou/update.jsp</result>
			<result name="forAdd">/customer/guangzhou/input.jsp</result>
			<result name="deleteCustomer" type="redirectAction">./customer_guangzhou!searchCustomer.action</result>
			<result name="input" type="redirectAction">
				customer_guangzhou!doSomeForAddCustomer?from=guangzhou&amp;deleteSession=input
			</result>
			<result name="table">/customer/guangzhou/table.jsp</result>
		</action>
		
	</package>
	
	<!-- 广州项目登陆成功后的主页 -->
	<package name="customer_guangzhou_index" extends="struts-default" namespace="/customer_guangzhou/index">
		<action name="login_easyui" class="com.ihk.customer.action.GuangZhouIndexAction" method="login">
		
			<!-- <result name="loginSucc">/customer/guangzhou/index_login.jsp</result>-->
			<result name="loginSucc">/ui/index.jsp</result> 
		</action>
		
		<action name="selectProject" class="com.ihk.customer.action.GuangZhouIndexAction" method="selectProjectByRole">
		<!-- 
			<result name="loginSucc">/customer/guangzhou/index_login.jsp</result> -->
			<result name="loginSucc">/ui/index.jsp</result>
			
		</action>
	</package>
	
	<package name="customer_guangzhou_input" extends="struts-default" namespace="/customer_guangzhou/input">
		<!-- 跳到增加客户界面 -->
		<action name="forAdd" class="com.ihk.customer.action.GuangZhouInputAction" method="doSomeForAddCustomer1">
			<result name="forAdd">/customer/guangzhou/input1.jsp</result>
			<result name="forHuiJingAdd">/customer/guangzhou/input_huijing.jsp</result>
			<result name="forQiaoXinAdd">/customer/guangzhou/input_qiaoxin.jsp</result>
			<result name="forQuestionAdd">/customer/guangzhou/input_by_question.jsp</result>
			<result name="forShanYuhu">/customer/guangzhou/input1_shanyuhu.jsp</result>
		</action>
		<!-- 增加客户 -->
		<action name="addCustomer" class="com.ihk.customer.action.GuangZhouInputAction" method="addCustomer">
			<interceptor-ref name="defaultStack"/>
			<interceptor-ref name="token"/>
			<result name="invalid.token" type="redirectAction">
				forAdd?from=guangzhou&amp;deleteSession=token
			</result>
			<result name="customerAdd" type="redirectAction">
				forAdd?from=guangzhou&amp;deleteSession=false
			</result>
		</action>
		
		<!-- 验证问卷必填项 -->
		<action name="validateQuestion" class="com.ihk.customer.action.GuangZhouInputAction" method="validateQuestion" />
		
		<!-- 增加客户+问卷 -->
		<action name="addCustomerAndQuestion" class="com.ihk.customer.action.GuangZhouInputAction" method="addCustomerAndQuestion">
			<result name="suc" type="redirectAction">
				forAdd?from=guangzhou&amp;deleteSession=false&amp;customerName=1
			</result>
			<result name="fail">/customer/guangzhou/input1.jsp</result>
		</action>
		
		<!-- 客户问卷 -->
		<action name="findQuestionTable" class="com.ihk.customer.action.GuangZhouInputAction" method="findTocList">
			<result name="question">/customer/guangzhou/input_question_table.jsp</result>
			<result name="old">/customer/guangzhou/input_old_project_buttom.jsp</result>
		</action>
		
		<!-- 客户基本问卷 -->
		<action name="getBaseQuestion" class="com.ihk.customer.action.GuangZhouInputAction" method="getBaseQuestion">
			<result name="success">/customer/guangzhou/base_question.jsp</result>
		</action>
		
		<!-- 带答案客户问卷 -->
		<action name="findAnswerTable" class="com.ihk.customer.action.GuangZhouInputAction" method="findAnswerList">
			<result name="question">/customer/guangzhou/input_question_table.jsp</result>
			<result name="answer">/customer/guangzhou/update_question_table.jsp</result>
			<result name="old">/customer/guangzhou/input_old_project_buttom.jsp</result>
		</action>
		
		<!-- 新客户问卷多份 -->
		<action name="findQuestionList" class="com.ihk.customer.action.GuangZhouInputAction" method="findQuestionList">
			<result name="question">/customer/guangzhou/input_question_table.jsp</result>
			<result name="old">/customer/guangzhou/input_old_project_buttom.jsp</result>
		</action>
		<!-- 得到售前客户问卷的列表(项目) -->
		<action name="pickQuestionList" class="com.ihk.customer.action.GuangZhouInputAction" method="pickQuestionList"/>
		<!-- 得到售前客户问卷的列表(公司) -->
		<action name="pickQuestionListCompany" class="com.ihk.customer.action.GuangZhouInputAction" method="pickQuestionListCompany"/>
		
		<!-- 为了保证使用固定新建用户和自定义新建用户项目  进行选择的项目验证 
		
		<action name="changeProject" class="com.ihk.customer.action.GuangZhouInputAction" method="changeProject">
		</action>-->
	</package>
	
	<!-- 删除客户 -->
	<package name="customer_guangzhou_delete" extends="struts-default" namespace="/customer_guangzhou/delete">
		<action name="customers" class="com.ihk.customer.action.GuangZhouDeleteAction" method="delCustomers">
			<result name="deleteCustomer" type="chain">
				<param name="namespace">/customer_guangzhou/search</param>
				<param name="actionName">indexForNotDeleSugg</param>
			</result>
		</action>
	</package>
	
	<!-- 恢复客户-->
	<package name="customer_guangzhou_recover" extends="struts-default" namespace="/customer_guangzhou/recover">
		<action name="customers" class="com.ihk.customer.action.GuangZhouRecoverAction" method="recoverCustomers">
				<result name="recoverCustomer" type="chain">
					<param name="namespace">/customer_guangzhou/search</param>
					<param name="actionName">indexForNotDeleSugg</param>
				</result>
		</action>
	</package>
	
	<!-- 复制(转移)客户 -->
	<package name="customer_guangzhou_copy" extends="struts-default" namespace="/customer_guangzhou/copy">
		<action name="customers" class="com.ihk.customer.action.GuangZhouCopyCustomerAction" method="copyCustomers">
			<result name="copyCustomer" type="chain">
				<param name="namespace">/customer_guangzhou/search</param>
				<param name="actionName">indexForNotDeleSugg</param>
			</result>
		</action>
	</package>
	
	<!-- 设置只能查看 -->
	<package name="customer_only_follow" extends="struts-default" namespace="/customer_guangzhou/only_follow">
		<action name="customers" class="com.ihk.customer.action.GuangZhouOnlyFollowCustomerAction" method="onlyFollowCustomers">
			<result name="onlyFollowCustomers" type="chain">
				<param name="namespace">/customer_guangzhou/search</param>
				<param name="actionName">indexForNotDeleSugg</param>
			</result>
		</action>
	</package>
	
	<!-- 基于easyui的客源设置 start -->
	<package name="customer_only_follow_easyui" extends="struts-default" namespace="/customer/only_follow/easyui">
		<action name="toSetUpOnlyFollow" class="com.ihk.customer.action.easyui.OnlyFollowCustomerEasyUIAction" method="toSetUpOnlyFollow">
			<result name="toSetUpOnlyFollow">/customer/easyui/setup_follow.jsp</result>
		</action>
		
		<action name="setUpOnlyFollow" class="com.ihk.customer.action.easyui.OnlyFollowCustomerEasyUIAction" method="setUpOnlyFollow"/>
		
		<action name="deleteOnlyFollow" class="com.ihk.customer.action.easyui.OnlyFollowCustomerEasyUIAction" method="deleteOnlyFollow"/>
		
	</package>
	<!-- 基于easyui的客源设置 end -->
	
	<package name="customer_guangzhou_update" extends="struts-base" namespace="/customer_guangzhou/update">
		<!-- 查找一个客户 -->
		<action name="getById" class="com.ihk.customer.action.GuangZhouUpdateAction" method="queryCustomerById">
			<result name="queryCustomerByIdForUpdate">/customer/guangzhou/update.jsp</result>
			<result name="queryHuiJingCustomerByIdForUpdate">/customer/guangzhou/update_huijing.jsp</result>
			<result name="queryQiaoXinCustomerByIdForUpdate">/customer/guangzhou/update_qiaoxin.jsp</result>
			<result name="queryCustomerAndQuestionByIdForUpdate">/customer/guangzhou/update_by_question1.jsp</result>
			<result name="queryShanYuHuCustomerByIdForUpdate">/customer/guangzhou/update_by_shanyuhu.jsp</result>
		</action>
		<!-- 更新客户 -->
		<action name="toUpdate" class="com.ihk.customer.action.GuangZhouUpdateAction" method="updateCustomer">
			<result name="queryCustomerById" type="redirectAction">getById.action</result>
		</action>
		
		<!-- 验证问卷必填项 -->
		<action name="validateQuestion" class="com.ihk.customer.action.GuangZhouUpdateAction" method="validateQuestion" />
		
		<!-- 更新客户+问卷 -->
		<action name="toUpdateQuestion" class="com.ihk.customer.action.GuangZhouUpdateAction" method="updateCustomerAndQuestion">
			<result name="queryCustomerById" type="redirectAction">getById.action</result>
		</action>
		
		
		<!-- 更新最新问卷 -->
		<action name="addAnwser" class="com.ihk.customer.action.GuangZhouUpdateAction" method="updateQuestion">
			<result name="suc" type="redirectAction">getById.action</result>
		</action>
	</package>
	
	<package name="guangzhou_customer_search" extends="struts-default" namespace="/customer_guangzhou/search">
		<!-- 查找客户 -->
		<action name="index" class="com.ihk.customer.action.GuangZhouSearchAction" method="index">
			<result name="searchCustomer" >/customer/guangzhou/index.jsp</result>
		</action>
		<!-- 获取项目对应付款方式列表 -->
		<action name="getSelPayWayByCompanyProjectId" class="com.ihk.customer.action.GuangZhouSearchAction" method="getPayTypeListByCompanyProjectId">
		</action>
		
		<!-- 查找客户并删除提示 -->
		<action name="indexForDeleSugg" class="com.ihk.customer.action.GuangZhouSearchAction" method="indexForDeleSugg">
			<result name="indexForDeleSugg" type="redirectAction">index?deleteSugg=true</result>
		</action>
		<!-- 查找客户不删除提示 -->
		<action name="indexForNotDeleSugg" class="com.ihk.customer.action.GuangZhouSearchAction" method="indexForNotDeleSugg">
			<result name="indexForNotDeleSugg" type="redirectAction">index?deleteSugg=false</result>
		</action>
		<!-- 查找客户(用于查找按钮) -->
		<action name="search" class="com.ihk.customer.action.GuangZhouSearchAction" method="search">
			<result name="searchCustomer" >/customer/guangzhou/index.jsp</result>
		</action>
		<!-- 查找客户不删除提示(用于查找按钮) -->
		<action name="searchForNotDelSugg" class="com.ihk.customer.action.GuangZhouSearchAction" method="searchForNotDelSugg">
			<result name="searchForNotDelSugg" type="redirectAction">searchOrderBy?deleteSugg=false</result>
		</action>
		<!-- 查找客户(排序) -->
		<action name="searchOrderBy" class="com.ihk.customer.action.GuangZhouSearchAction" method="searchOrderBy">
			<result name="searchCustomer" >/customer/guangzhou/index.jsp</result>
		</action>
		<!-- 查找售后客户(排序) -->
		<action name="searchContractCustomerOrderBy" class="com.ihk.customer.action.GuangZhouSearchAction" method="searchContractCustomerOrderBy">
			<result name="searchContractCustomer" >/customer/guangzhou/indexContractCustomer.jsp</result>
		</action>
		<!-- 下载客户资料 -->
		<action name="downLoad" class="com.ihk.customer.action.GuangZhouSearchAction" method="downLoad"/>
		<!-- 下载售后客户资料 -->
		<action name="downLoadContractCustomer" class="com.ihk.customer.action.GuangZhouSearchAction" method="downLoadContractCustomer"/>
		<!-- 查找售后客户 -->
		<action name="indexContractCustomer" class="com.ihk.customer.action.GuangZhouSearchAction" method="indexContractCustomer">
			<result name="searchContractCustomer" >/customer/guangzhou/indexContractCustomer.jsp</result>
		</action>
		<!-- 查找售后客户按钮 -->
		<action name="searchContractCustomer" class="com.ihk.customer.action.GuangZhouSearchAction" method="searchContractCustomer">
			<result name="searchContractCustomer" >/customer/guangzhou/indexContractCustomer.jsp</result>
		</action>
	</package>
	
	
	<package name="customer_guangzhou_follow" extends="struts-default" namespace="/customer_guangzhou/follow">
		<!-- 跳到跟进界面(列表界面的操作) -->
		<action name="toAdd" class="com.ihk.customer.action.GuangZhouFollowAction" method="getSomeForAddFollow"/>
		<!-- 跳到跟进界面(修改界面的操作) -->
		<action name="toAddIndex" class="com.ihk.customer.action.GuangZhouFollowAction" method="getSomeForAddIndexFollow"/>
		<!-- 增加跟进(列表界面的操作) -->
		<action name="add" class="com.ihk.customer.action.GuangZhouFollowAction" method="addFollow">
			<result name="queryCustomerById" type="chain">
				<param name="namespace">/customer_guangzhou/update</param>
				<param name="actionName">getById</param>
				<param name="method">queryCustomerById</param>
			</result>
		</action>
		<!-- 增加跟进(修改界面的操作) -->
		<action name="addFollowForIndex" class="com.ihk.customer.action.GuangZhouFollowAction" method="addFollowForIndex">
			<result name="addFollowForIndex" type="chain">
				<param name="namespace">/customer_guangzhou/search</param>
				<param name="actionName">searchForNotDelSugg</param>
				<param name="method">searchForNotDelSugg</param>
			</result>
		</action>
		
	</package>
	
	<package name="guangzhou" extends="struts-default" namespace="/customer_guangzhou/search">
		<!-- 根据省获取对应的市 -->
		<action name="proviToCity" class="com.ihk.customer.action.ProviCityRegionAction" method="getCityByProvinceId"/>
		<!-- 根据市获取对应的地区 -->
		<action name="cityToRegion" class="com.ihk.customer.action.ProviCityRegionAction" method="getRegionByCityId"/>
		<!-- 该方法已经废弃,该为手填 -->
		<action name="regionToBlock" class="com.ihk.customer.action.ProviCityRegionAction" method="getBlockByRegionId"/>
		
		<!-- 所属销售 -->
		<action name="sales" class="com.ihk.customer.action.SaleListAction" method="sales"/>
		
		<!-- 左边项目智能提示框 -->
		<action name="leftProjects" class="com.ihk.customer.action.ProjectListAction" method="leftProjects"/>
		
		<!-- 左边项目的easyui -->
		<action name="leftProjectsEasyui" class="com.ihk.customer.action.ProjectListAction" method="leftProjectsEasyui"/>
		
		<!-- 广州公司实际项目 top -->
		<action name="projects" class="com.ihk.customer.action.ProjectListAction" method="searchProject"/>
		<!-- 根据项目名精确查找 -->
		<action name="searchProjectByExactName" class="com.ihk.customer.action.ProjectListAction" method="searchProjectByExactName"/>
		<!-- 判断项目是否存在 -->
		<action name="projectIsExists" class="com.ihk.customer.action.ProjectListAction" method="projectIsExists"/>
		<!-- 根据项目id获取标红字段 -->
		<action name="getRedsByProjectId" class="com.ihk.customer.action.ProjectListAction" method="getRedsByProjectId"/>
		<!-- 根据项目名称获取对应的id -->
		<action name="getProjectIdByProjectName" class="com.ihk.customer.action.ProjectListAction" method="getProjectIdByProjectName"/>
		<!-- end -->
		
		<!-- 广州公司楼盘项目top -->
		<action name="propertyProjects" class="com.ihk.customer.action.ProjectListAction" method="searchPropertyProject"/>
		<!-- 根据楼盘项目名称获取对应的id -->
		<action name="getProjectIdByProjectNameForProperty" class="com.ihk.customer.action.ProjectListAction" method="getProjectIdByProjectNameForProperty"/>
		<!--  
		<action name="propertyAreas" class="com.ihk.customer.action.ProjectListAction" method="searchPropertyProject"/>
		
		<action name="getAreaIdByAreaNameForArea" class="com.ihk.customer.action.ProjectListAction" method="getProjectIdByProjectNameForProperty"/>
		-->
		<!-- 根据楼盘项目id获取对应的楼栋(用于ajax) -->
		<action name="getBuildListFromPropertyId" class="com.ihk.customer.action.ProjectListAction" method="getBuildListFromPropertyId"/>
		<!-- 根据楼盘项目id获取对应的楼栋(用于智能提示框) -->
		<action name="getBuildListFromPropertyIdForAuto" class="com.ihk.customer.action.ProjectListAction" method="getBuildListFromPropertyIdForAuto"/>
		<!-- 根据单元id获取页面其他项目 -->
		<action name="getOtherDataByUnitId" class="com.ihk.customer.action.ProjectListAction" method="getOtherDataByUnitId"/>
		
		<!-- end -->
		
		<!-- 广州公司BUILD -->
		<!-- 查找楼栋 -->
		<action name="propertyBuild" class="com.ihk.customer.action.ProjectListAction" method="searchPropertyBuild"/>
		<!-- 根据楼栋名称获取对应的id -->
		<action name="getBuildIdByBuildName" class="com.ihk.customer.action.ProjectListAction" method="getBuildIdByBuildNameForProperty"/>
		
		<!-- 获取家庭收入下拉框 -->
		<action name="getSelFamilyIncome" class="com.ihk.customer.action.SelFamilyIncome" method="getSelFamilyIncome"/>
		<!-- 根据项目id获取对应的家庭收入 -->
		<action name="getSelFamilyIncomeByProjectId" class="com.ihk.customer.action.SelFamilyIncome" method="getSelFamilyIncomeByProjectId"/>
		
		<!-- 获取客户评级 -->
		<action name="getSelRating" class="com.ihk.customer.action.SelRating" method="getSelRating"/>
		<!-- 根据项目id获取对应的客户评级 -->
		<action name="getSelRatingByProjectId" class="com.ihk.customer.action.SelRating" method="getSelRatingByProjectId"/>
		
		<!-- 根据项目id获取默认的居住工作区域的省市 -->
		<action name="getSelProjectDefaultProvince" class="com.ihk.customer.action.SelProjectDefaultProvinceAndCity" method="getSelProjectDefaultProvince"/>
		<action name="getSelProjectDefaultCity" class="com.ihk.customer.action.SelProjectDefaultProvinceAndCity" method="getSelProjectDefaultCity"/>
		<action name="getSelProjectDefaultRegion" class="com.ihk.customer.action.SelProjectDefaultProvinceAndCity" method="getSelProjectDefaultRegion"/>
		
		<!-- 根据项目id和分析类型,获取区间范围的值(没使用到) -->
		<action name="getRangeFromProjectIdAndType" class="com.ihk.customer.action.GetRangeFromProjectIdAndTypeAction" method="getRangeFromProjectIdAndType"/>
		<!-- 判断号码是否存在 -->
		<action name="phoneIsExists" class="com.ihk.customer.action.PhoneAction" method="findPhoneIsExists"/>
		<!-- 判断家庭号码是否存在 -->
		<action name="homePhoneIsExists" class="com.ihk.customer.action.PhoneAction" method="findHomePhoneIsExists"/>
		<!-- 根据map参数判断号码是否存在 -->
		<action name="phoneIsExistsForMap" class="com.ihk.customer.action.PhoneAction" method="findPhoneIsExistsForMap"/>
		<!-- 根据map参数判断家庭号码是否存在 -->
		<action name="homePhoneIsExistsForMap" class="com.ihk.customer.action.PhoneAction" method="findHomePhoneIsExistsForMap"/>
		<!-- 查找用户(用于智能提示框) -->
		<action name="users" class="com.ihk.customer.action.UserListAction" method="searchUser"/>
		
		<!-- appoint customer start(预约客户) -->
		<!-- 跳到增加 -->
		<action name="toAddAppointCustomer" class="com.ihk.customer.action.UserListAction" method="toAddAppointCustomer"/>
		<!-- 显示单个客户 -->
		<action name="showAppointCustomer" class="com.ihk.customer.action.UserListAction" method="showAppointCustomer"/>
		<!-- 增加 -->
		<action name="addAppointCustomer" class="com.ihk.customer.action.UserListAction" method="addAppointCustomer"/>
		<!-- 获取单个客户 -->
		<action name="getAppointCustomerById" class="com.ihk.customer.action.UserListAction" method="getAppointCustomerById"/>
		<!-- 查找客户列表 -->
		<action name="appointCustomers" class="com.ihk.customer.action.UserListAction" method="searchAppointCustomer"/>
		<!-- appoint customer end -->
		
		<!-- confirm customer start(认购,合同的操作合并) -->
		<!-- 跳到增加 -->
		<action name="toAddConfirmCustomer" class="com.ihk.customer.action.UserListAction" method="toAddConfirmCustomer"/>
		<!-- 显示单个客户 -->
		<action name="showConfirmCustomer" class="com.ihk.customer.action.UserListAction" method="showConfirmCustomer"/>
		<!-- 增加 -->
		<action name="addConfirmCustomer" class="com.ihk.customer.action.UserListAction" method="addConfirmCustomer"/>
		<!-- 获取单个客户 -->
		<action name="getConfirmCustomerById" class="com.ihk.customer.action.UserListAction" method="getConfirmCustomerById"/>
		<!-- 查找客户列表(认购/签约) -->
		<action name="confirmCustomers" class="com.ihk.customer.action.UserListAction" method="searchConfirmCustomer"/>
		<!-- 查找客户列表(合同) -->
		<action name="contractCustomers" class="com.ihk.customer.action.UserListAction" method="searchContractCustomer"/>
		<!-- confirm customer end -->
		<!-- 根据名称获取用户id -->
		<action name="getUserIdByName" class="com.ihk.customer.action.UserListAction" method="getUserIdByName"/>
		<!-- 根据楼栋id获取单元列表 -->
		<action name="searchUnitListByBuildId" class="com.ihk.property.action.PropertyAjaxAction" method="searchUnitListByBuildId"/>
		<!-- 获取号码归属地(已经废弃),可以作为手工修改的如果,如果在本地要修改数据库连接 ./customer_guangzhou/search/tmpFetchPhoneFrom.action-->
		<action name="tmpFetchPhoneFrom" class="com.ihk.customer.action.CustomerAction" method="tmpFetchPhoneFrom"/>
		
	</package>
	
	

	<!-- set_role_byus   设置用户权限  已经没有使用的    (在重新设计权限之后就没有再使用)-->
	<package name="guangzhou_role" extends="struts-default" namespace="/guangzhou/role">
		<action name="index" class="com.ihk.sale.action.SetUserRoleByUserAction" method="search">
			<result name="setrole">/customer/guangzhou/set_userRole_byUser.jsp</result>
		</action>
		<action name="search" class="com.ihk.sale.action.SetUserRoleByUserAction" method="search">
			<result name="setrole">/customer/guangzhou/set_userRole_byUser.jsp</result>
		</action>
		<action name="add" class="com.ihk.sale.action.SetUserRoleByUserAction" method="addRole">
			<result name="setrole">/customer/guangzhou/set_userRole_byUser.jsp</result>
		</action>
		<action name="del" class="com.ihk.sale.action.SetUserRoleByUserAction" method="delRole">
			<result name="setrole">/customer/guangzhou/set_userRole_byUser.jsp</result>
		</action>
