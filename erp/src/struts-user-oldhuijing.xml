<struts>
	<!-- 最初 汇景新城  现在已经迁移 没有使用 2012/09/27-->
	<package name="user_oldhuijing" extends="struts-default">
		<action name="user_oldhuijing!*" class="com.ihk.user.action.UserAccountAction">
			<result name="error">/userAccount/oldhuijing/error.jsp</result>
			<result name="loginUserAccount" type="redirectAction">${actionFront}!searchCustomer.action</result>
			<result name="newUserAccount">/userAccount/oldhuijing/success.jsp</result>
			<result name="findUserAccountPage">/userAccount/oldhuijing/index.jsp</result>
			<result name="indexUserAccount">/userAccount/oldhuijing/index.jsp</result>
			<result name="addUserAccountJsp">/userAccount/oldhuijing/input.jsp</result>
			<result name="addUserAccount">/userAccount/oldhuijing/success.jsp</result>
			<result name="updateUserAccountJsp_user">/userAccount/oldhuijing/update_user.jsp</result>		
			<result name="updateUserAccountJsp_manager">/userAccount/oldhuijing/update_manager.jsp</result>
			<result name="updateByManagerJsp">/userAccount/oldhuijing/update_manager.jsp</result>		
			<result name="updatePwdByManager">/userAccount/oldhuijing/success.jsp</result>		
			<result name="updatePwd">/userAccount/oldhuijing/success.jsp</result>		

		</action>
		
	</package>

</struts>
